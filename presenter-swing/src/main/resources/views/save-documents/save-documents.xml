<?xml version="1.0" encoding="UTF-8"?>
<Panel name="save.documents.title">
	<Panel layout="GridBagLayout" border="EmptyBorder(0, 20, 20, 20)">
		<Label text="save.documents.select.individual" font="Arial 22" ipady="10"  border="EmptyBorder(0, 0, 10, 0)">
			<gridbagconstraints gridx="0" gridy="0" fill="GridBagConstraints.HORIZONTAL" />
		</Label>
		<HBox id="individualContainer" gap="5" >
			<gridbagconstraints gridx="0" gridy="1" gridwidth="2" anchor="GridBagConstraints.WEST" fill="GridBagConstraints.HORIZONTAL" />
		</HBox>

		<HBox gap="5" border="EmptyBorder(10, 0, 0, 0)">
			<gridbagconstraints gridx="0" gridy="2" gridwidth="2"  ipady="20" fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.SOUTHEAST" />
			<box.hglue />
			<Button id="closeButton" ipady="10"  font="Arial 20"  font-scale="1.25"  text="button.close" />
		</HBox>
	</Panel>
</Panel>